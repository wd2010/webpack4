  "version": "1.0.0",
  "description": "基于webpack4的网站",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_BUILD=client NODE_ENV=development webpack-dev-server --hotOnly --progress --config ./config/webpack.dev.config.js ",
